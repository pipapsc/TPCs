  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def distribPorCurso():\n",
    "    bd = lerDataset()\n",
    "    distribuicao = {}\n",
    "    for a in bd:\n",
    "        if a[2] in distribuicao.keys():\n",
    "            distribuicao[a[2]] = distribuicao[a[2]]+1\n",
    "        else:\n",
    "            distribuicao[a[2]] = 1\n",
    "    return distribuicao #{curso : numero de alunos}"
